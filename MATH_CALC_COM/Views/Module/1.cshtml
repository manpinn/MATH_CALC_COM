
 
<div id="chartDiv"></div>


<script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
<script type="text/javascript">
    var chartJson = '@Html.Raw(ViewData["ChartJson"])';
    var chartData = JSON.parse(chartJson);
    var layout = {
        title: 'Scroll and Zoom'
    };
    Plotly.newPlot('chartDiv', chartData.Fields, layout, { responsive: true, scrollZoom: true, displayModeBar: true });
</script>  